<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="default" extends="hibernate-default">
		<action name="alta" class="org.action.alta.ActionAlta"
			method="addPRODUCTO_T">
			<result name="input">/formalta.jsp</result>
			<result name="success">/index.jsp</result>
		</action>
		<action name="consulta" class="org.action.consulta.ActionConsulta"
			method="selectPRODUCTO_T">
			<result name="success">/formconsulta.jsp</result>
		</action>
		<action name="catalogo" class="org.action.catalogo.ActionCatalogo"
			method="catalogo">
			<result name="success">/formcatalogo.jsp</result>
		</action>
	</package>
</struts>